generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model messages {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model users {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  v         Int    @map("__v")
  email     String @unique(map: "email_1")
  firstName String
  lastName  String
  password  String
}
